{
  "extends": [
    "next/core-web-vitals",
    "plugin:promise/recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "rules": {
    "no-console": 2,
    "no-debugger": 2,
    "for-direction": 2,
    "getter-return": 2,
    "no-compare-neg-zero": 2,
    "no-cond-assign": 2,
    "no-control-regex": 2,
    "no-dupe-args": 2,
    "no-dupe-else-if": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 1,
    "no-empty-character-class": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-import-assign": 2,
    "no-invalid-regexp": [2, { "allowConstructorFlags": ["u", "y"] }],
    "no-regex-spaces": 1,
    "no-setter-return": 2,
    "no-sparse-arrays": 0,
    "no-template-curly-in-string": 0,
    "no-unexpected-multiline": 0,
    "no-unreachable": 2,
    "no-unsafe-finally": 0,
    "no-unsafe-negation": 0,
    "use-isnan": 2,
    "valid-typeof": 2,
    "accessor-pairs": 2,
    "class-methods-use-this": 1,
    "complexity": 1,
    "consistent-return": 0,
    "curly": [2, "multi", "consistent"],
    "default-param-last": 0,
    "dot-location": ["error", "property"],
    "eqeqeq": [2, "always"],
    "max-classes-per-file": [1, 1],
    "no-alert": 1,
    "no-case-declarations": 0,
    "no-constructor-return": 2,
    "no-else-return": 2,
    "no-empty-function": 1,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extra-label": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-implied-eval": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-multi-spaces": 2,
    "no-octal": 2,
    "no-param-reassign": 0,
    "no-redeclare": 1,
    "no-self-compare": 2,
    "no-unused-labels": 2,
    "no-useless-call": 2,
    "no-useless-catch": 1,
    "no-useless-concat": 2,
    "no-useless-escape": 2,
    "no-useless-return": 1,
    "no-with": 2,
    "require-await": 2,
    "vars-on-top": 1,
    "init-declarations": [1, "always"],
    "no-label-var": 2,
    "no-shadow-restricted-names": 2,
    "no-undef": 2,
    "no-undef-init": 2,
    "no-undefined": 2,
    "no-unused-vars": 2,
    "no-use-before-define": 2,
    "array-bracket-newline": [2, { "multiline": true }],
    "array-bracket-spacing": [2, "never"],
    "array-element-newline": [0, { "multiline": true, "minItems": 5 }],
    "block-spacing": [2, "always"],
    "brace-style": 2,
    "camelcase": 1,
    "comma-dangle": [2, "only-multiline"],
    "comma-spacing": [2, { "before": false, "after": true }],
    "computed-property-spacing": [2, "never"],
    "eol-last": [2, "always"],
    "func-call-spacing": [2, "never"],
    "jsx-quotes": [2, "prefer-single"],
    "lines-between-class-members": [2, "always"],
    "new-cap": [2, { "newIsCap": true }],
    "new-parens": 2,
    "newline-per-chained-call": [2, { "ignoreChainWithDepth": 2 }],
    "no-lonely-if": 2,
    "no-mixed-spaces-and-tabs": 0,
    "no-multiple-empty-lines": [2, { "max": 2, "maxEOF": 1 }],
    "no-trailing-spaces": 2,
    "no-unneeded-ternary": 2,
    "no-whitespace-before-property": 2,
    "object-curly-spacing": [2, "always"],
    "one-var-declaration-per-line": [2, "always"],
    "padded-blocks": ["error", "never"],
    "semi-style": [2, "last"],
    "space-infix-ops": 2,
    "spaced-comment":[0, "never"],
    "switch-colon-spacing": ["error", {"after": true, "before": false}],
    "constructor-super": 2,
    "generator-star-spacing": [2, {"before": false, "after": true}],
    "no-class-assign": 2,
    "no-const-assign": 2,
    "no-dupe-class-members": 2,
    "no-duplicate-imports": 2,
    "no-this-before-super": 2,
    "no-useless-computed-key": 2,
    "no-useless-constructor": 2,
    "no-useless-rename": 2,
    "no-var": 1,
    "prefer-arrow-callback": 1,
    "prefer-const": 2,
    "prefer-rest-params": 1,
    "prefer-spread": 2,
    "prefer-template": 2,
    "require-yield": 2,
    "no-img-element": 0,
    "react-hooks/exhaustive-deps":  2,
    "@next/next/no-script-in-head": 0,
    "@next/next/inline-script-id": 0,
    "@next/next/no-img-element": 0,
    "prettier/prettier": [
      "error",
      {
        "useTabs": true,
        "printWidth": 120,
        "tabWidth": 4,
        "singleQuote": true,
        "quoteProps": "as-needed",
        "trailingComma": "es5",
        "bracketSpacing": true,
        "jsxBracketSameLine": false,
        "jsxSingleQuote": true,
        "parser": "typescript",
        "semi": true
      }
    ]
  },
  "parserOptions": {
    "warnOnUnsupportedTypeScriptVersion": false
  },
  "parser": "@typescript-eslint/parser",
  "env": {
    "browser": true,
    "es6": true,
    "node": true
 },
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "overrides": [
    {
      "files": [
        "**/*.ts",
        "**/*.tsx"
      ],
      "parser": "@typescript-eslint/parser",
      "plugins": [
        "@typescript-eslint"
      ],
      "rules": {
        "no-undef": 0,
        "curly": 0,
        "@typescript-eslint/no-var-requires": 1,
        "@typescript-eslint/ban-ts-comment": 0,
        "@typescript-eslint/no-explicit-any": 0,
        "@typescript-eslint/no-unused-vars": 1,
        "promise/catch-or-return": 0,
        "promise/always-return": 0,
        "no-unused-vars": 1
      }
    }
  ]
}

